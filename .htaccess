<IfModule mod_rewrite.c>
RewriteEngine On
RewriteCond $1 !^(index\.php|resources|robots\.txt)
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^\/(.*)$ index.php/$1 [L,QSA]
</IfModule>

Options All -Indexes

<Files .htaccess>
Order Allow,Deny
Deny from all
</Files>

